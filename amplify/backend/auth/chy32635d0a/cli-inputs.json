    "rules": [
      {
        "provider": "user",
        "name": "customRule",
        "enabled": true,
        "description": "Allow registration only for users with email ending in '@cpp.edu'",
        "attributeCheck": {
          "email": [
            {
              "equals": "@cpp.edu",
              "operator": "endsWith"
            }
          ]
        },
        "actions": {
          "denyRegistration": {
            "challengeName": "CUSTOM_CHALLENGE",
            "challengeMetadata": "Invalid email domain. Please use an email ending in '@cpp.edu'."
          }
        }
      }
    ],
    "triggers": {
      "preSignUp": "arn:aws:lambda:us-west-1:001413739527:function:preSignUp-dev"
